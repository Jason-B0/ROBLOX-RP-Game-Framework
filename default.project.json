{
  "name": "Rojo_RoRPFramework",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "$path": "src/shared",
      "Packages": {
        "$path": "Packages"
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$path": "src/server"
    },
    
    "ServerStorage": {
      "$className": "ServerStorage",
      "$path": "src/storage"
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "$path": "src/client",
      "StarterCharacterScripts": {
        "$className": "StarterCharacterScripts",
        "$path": "src/client/characterScript"
      },
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "$path": "src/client/playerScript"
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}